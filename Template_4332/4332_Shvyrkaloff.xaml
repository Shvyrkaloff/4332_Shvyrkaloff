<Window x:Class="Template_4332._4332_Shvyrkaloff"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4332"
        mc:Ignorable="d"
        Title="_4332_Shvyrkaloff" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Audi 100 C3 Sport Exklusiv S743</Label>
        <Label Grid.Row="1">Возраст: 32</Label>
        <Label Grid.Row="2">Двигатель: МС2 (165 л.с., рабочий объем: 2.23 л., степень сжатия: 8.4, октановое число бензина: 95, диаметр цилиндров: 81 мм)</Label>
        <StackPanel Grid.Row="3" Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
            <Button x:Name="ExportForExcelButton" Margin="10" Width="250" Click="ExportForExcelButton_OnClick">Экспорт в Excel</Button>
            <Button x:Name="ImportButton" Margin="10" Width="250" Click="ImportButton_OnClick">Импорт данных</Button>
        </StackPanel>
    </Grid>
</Window>
